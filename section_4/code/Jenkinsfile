#!/usr/bin/groovy
pipeline {
	agent any
	parameters {
        	string(name: 'OracleVersion', defaultValue: '12', description: 'How should I greet the world?')
    	}
	stages {

		stage('Build') {
			steps {
				echo 'Building..${params.OracleVersion}'
			}
		}

		stage('Test') {
			steps {
				echo 'Testing..'
			}
		}

		stage('Deploy') {
			steps {
				echo 'Deploying....'
			}
		}

	}
}
